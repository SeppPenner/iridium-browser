From 896bd4e2ff44aaa0851be94d7220eb8c9128ae7c Mon Sep 17 00:00:00 2001
From: Jan Engelhardt <jengelh@inai.de>
Date: Sun, 6 Sep 2020 20:35:35 +0200
Subject: [PATCH 07/76] Set build name

---
 chrome/app/theme/chromium/BRANDING | 14 +++++++-------
 chrome/common/chrome_constants.cc  |  4 ++--
 components/version_ui_strings.grdp |  2 +-
 3 files changed, 10 insertions(+), 10 deletions(-)

diff --git chrome/app/theme/chromium/BRANDING chrome/app/theme/chromium/BRANDING
index a2020a0b6f927..58eb0d27a4ccb 100644
--- chrome/app/theme/chromium/BRANDING
+++ chrome/app/theme/chromium/BRANDING
@@ -1,10 +1,10 @@
-COMPANY_FULLNAME=The Chromium Authors
-COMPANY_SHORTNAME=The Chromium Authors
-PRODUCT_FULLNAME=Chromium
-PRODUCT_SHORTNAME=Chromium
-PRODUCT_INSTALLER_FULLNAME=Chromium Installer
-PRODUCT_INSTALLER_SHORTNAME=Chromium Installer
-COPYRIGHT=Copyright 2020 The Chromium Authors. All rights reserved.
+COMPANY_FULLNAME=The browser authors
+COMPANY_SHORTNAME=The browser authors
+PRODUCT_FULLNAME=Iridium
+PRODUCT_SHORTNAME=Iridium
+PRODUCT_INSTALLER_FULLNAME=Iridium Installer
+PRODUCT_INSTALLER_SHORTNAME=Iridium Installer
+COPYRIGHT=Copyright 2020 The browser authors.
 MAC_BUNDLE_ID=org.chromium.Chromium
 MAC_CREATOR_CODE=Cr24
 MAC_TEAM_ID=
diff --git chrome/common/chrome_constants.cc chrome/common/chrome_constants.cc
index bb7d9757bd038..1ac4543416ba0 100644
--- chrome/common/chrome_constants.cc
+++ chrome/common/chrome_constants.cc
@@ -11,11 +11,11 @@
 #define FPL FILE_PATH_LITERAL
 
 #if defined(OS_MACOSX)
-#define CHROMIUM_PRODUCT_STRING "Chromium"
+#define CHROMIUM_PRODUCT_STRING "Iridium"
 #if BUILDFLAG(GOOGLE_CHROME_BRANDING)
 #define PRODUCT_STRING "Google Chrome"
 #elif BUILDFLAG(CHROMIUM_BRANDING)
-#define PRODUCT_STRING "Chromium"
+#define PRODUCT_STRING "Iridium"
 #else
 #error Unknown branding
 #endif
diff --git components/version_ui_strings.grdp components/version_ui_strings.grdp
index 6278a8a380d60..ddc16a7988c8c 100644
--- components/version_ui_strings.grdp
+++ components/version_ui_strings.grdp
@@ -7,7 +7,7 @@
     Official Build
   </message>
   <message name="IDS_VERSION_UI_UNOFFICIAL" desc="unofficial build on the about:version page">
-    Developer Build
+    Iridium Build
   </message>
   <message name="IDS_VERSION_UI_32BIT" desc="32-bit on the chrome://version page">
     (32-bit)
-- 
